{
  "name": "query-builder-al",
  "version": "1.0.5",
  "description": "The queryBuilder function is a utility designed to construct and format URL query strings in JavaScript.",
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/mjs/index.js",
  "files": [
    "lib/**/*",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "yarn run clean && yarn run build:esm && yarn run build:cjs && yarn run transpile",
    "transpile": "babel lib/cjs --out-dir lib/cjs-transpiled",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "yarn run build",
    "test": "npx jest"
  },
  "keywords": [
    "url builder",
    "query maker",
    "javascript",
    "typescript",
    "esm",
    "cjs"
  ],
  "repository": "git@github.com:aleksandarLazic1998/findValueOfTheKey.git",
  "author": "aleksandarLazic1998 <aleksandar.lazic.p@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "query-builder-al": "^1.0.2",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  }
}
